# general

# list directory contents
alias lsa='ls -lah'
alias l='ls -lah'
alias ll='ls -lh'
alias la='ls -lAh'

alias md='mkdir -p'
alias rd=rmdir

alias 1='cd -'
alias 2='cd -2'
alias 3='cd -3'
alias 4='cd -4'
alias 5='cd -5'
alias 6='cd -6'
alias 7='cd -7'
alias 8='cd -8'
alias 9='cd -9'

alias grep='grep --color=auto'

alias ip="dig +short myip.opendns.com @resolver1.opendns.com"
alias wifipass="wifi-password"

# use openssl to generate a random password
# usage: genpass 16 (password with length of 16)
alias genpass="openssl rand -base64"


alias rm="trash" # always soft delete
alias subl="/Applications/Sublime\ Text.app/Contents/SharedSupport/bin/subl"

# git
# git alias already managed by oh-my-zsh git plugin
# https://github.com/robbyrussell/oh-my-zsh/wiki/Plugin:git


alias p="proxychains4"
alias pc="proxychains4"

# node.js
alias nr="npm run"
alias nis="npm install --save"
alias nid="npm install --save-dev"

# docker
alias d="docker"
alias dp="docker ps"
alias dpa="docker ps -a"
alias di="docker images"
alias dm="docker-machine"
alias dc="docker-compose"
