{
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.iconTheme": "material-icon-theme",
  "editor.tabSize": 2,
  "editor.fontSize": 14,
  "editor.formatOnPaste": false,
  "editor.wordWrap": "on",
  "editor.snippetSuggestions": "top",
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "gitProjectManager.searchInsideProjects": false,
  "gitProjectManager.baseProjectsFolders": ["/Users/VincentBel/dev/"],
  "gitProjectManager.ignoredFolders": ["node_modules"],
  "local-history.path": "/Users/VincentBel/dev/dotfiles/.vscode/",
  "prettier.eslintIntegration": true,
  "prettier.semi": false,
  "prettier.trailingComma": "es5",
  "prettier.singleQuote": true,
  "prettier.printWidth": 80,
  "vim.useSystemClipboard": true,
  "vim.leader": " ",
  "vim.overrideCopy": false,
  "vim.easymotion": true,
  "vim.otherModesKeyBindingsNonRecursive": [
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["<leader>", "g", "o"],
      "after": [],
      "commands": [
        {
          "command": "extension.openInGitHub",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "g", "d"],
      "after": [],
      "commands": [
        {
          "command": "git.openChange",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "l", "h"],
      "after": [],
      "commands": [
        {
          "command": "local-history.compareToActive",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "f", "d"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.formatDocument",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "f", "s"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.formatSelection",
          "args": []
        }
      ]
    },
    {
      // lint next
      "before": ["<leader>", "l", "n"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.marker.next",
          "args": []
        }
      ]
    },
    {
      // rename symbol
      "before": ["<leader>", "r", "s"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.rename",
          "args": []
        }
      ]
    },
    {
      // open markdown preview
      "before": ["<leader>", "m", "p"],
      "after": [],
      "commands": [
        {
          "command": "markdown.showPreviewToSide",
          "args": []
        }
      ]
    }
  ],
  "terminal.external.osxExec": "iTerm.app",
  "cSpell.userWords": [
    "frontmatter",
    "kubernetes",
    "lifecycle",
    "nginx",
    "polyfill"
  ],
  "cSpell.enabledLanguageIds": [
    "c",
    "cpp",
    "csharp",
    "go",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "markdown",
    "php",
    "plaintext",
    "python",
    "text",
    "typescript",
    "typescriptreact",
    "yml"
  ],
  "cSpell.showStatus": false,
  "importCost.bundleSizeDecoration": "gzipped",
  "docker.showExplorer": false,
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": true
  },
  "macros": {
    "commentAndCursorDown": [
      "editor.action.commentLine",
      "cursorDown"
    ]
  },
  "pasteImage.path": "./images",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "material-icon-theme.folders.theme": "classic",
  "material-icon-theme.folders.color": "#78909c",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.scss.d.ts": true
  },
  "workbench.editor.showTabs": true,
  "files.defaultLanguage": "markdown",
  "python.pythonPath": "/usr/local/bin/python3",
  "amVim.smartRelativeLineNumbers": true,
  "window.title": "${activeEditorMedium}",
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.statusBar.command": "gitlens.toggleFileBlame",
  "gitlens.settings.mode": "advanced",
  "gitlens.historyExplorer.location": "scm",
  "gitlens.currentLine.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.historyExplorer.enabled": true,
  "gitlens.codeLens.enabled": false,
  "workbench.tree.horizontalScrolling": true,
  "python.formatting.provider": "yapf",
}
